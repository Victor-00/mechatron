{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/login", "dest": "/api" },
    { "src": "/questions", "dest": "/api" },
    { "src": "/submit-quiz", "dest": "/api" },
    { "src": "/", "dest": "/api" },
    { "src": "/(.*)", "dest": "/api" }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
